for "_c" from 0 to 1 do {
	_spawnPos = [(_heliPos select 0) - (random 10), (_heliPos select 1) + (random 10) - 2, _heliPos select 2];
    (selectRandom [INFANTRY_PILOTS]) createUnit [[1,1,1], _heliGroup, "currentGuard = this", 0, (selectRandom [INFANTRY_RANK])];    
    waitUntil {alive currentGuard};    
    currentGuard setPosASL _spawnPos;
    currentGuard setDir (random 360);
    currentGuard setUnitPos "UP";
};
[(units _heliGroup)] call QS_fnc_setSkill3;
_heliGroup setVariable ["zbe_cacheDisabled", true, true];
_enemiesArray = _enemiesArray + (units _heliGroup);

_objects = nearestObjects [_flatPos, ["Land_Offices_01_V1_F","Land_i_Barracks_V2_F","Land_MilOffices_V1_F","Land_SCF_01_crystallizer_F","Land_SCF_01_diffuser_F","Land_SCF_01_chimney_F","Land_dp_bigTank_F","Land_MobileCrane_01_F","Land_GuardHouse_01_F","Land_Crane_F"], 200];
_supportPoses = [];
_hostagePoses = [];
_aaPoses = [];
_sniperPoses = [];
_viperPoses = [];
_guardPoses = [];
{
    _class = typeOf _x;
    _building = _x;
    switch (_class) do { 
   	    case "Land_Offices_01_V1_F": {   	        
            {
                _guardPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [0, 14];
            {
                _supportPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [18, 21];
            {
                _sniperPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [33, 35];
            {
                _aaPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [28, 30];
            {
                _hostagePoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [3, 4, 5, 6, 12, 13, 15, 19, 20, 22, 25, 26, 27];
   	    }; 
   	    case "Land_i_Barracks_V2_F": {
   	        {
                _guardPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [1, 3, 11, 13, 17, 28, 32, 35];
            {
                _supportPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [2, 4, 6, 8, 10, 16, 34, 38];
            {
                _sniperPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [34, 36, 39];
            {
                _aaPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [40, 41];
            {
                _hostagePoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [23, 24, 5, 7, 12, 20, 37];
   	    }; 
        case "Land_MilOffices_V1_F": {
            {
                _guardPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [1, 2, 3, 4, 8];
            {
                _supportPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [5];           
            {
                _hostagePoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [0, 6, 7];
   	    };
   	    case "Land_SCF_01_crystallizer_F": {
   	        {
                _viperPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [0, 1, 2, 3, 5, 8, 9, 10];
   	    };
   	    case "Land_SCF_01_diffuser_F": {
   	        {
                _guardPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [1, 6];
            {
                _aaPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [8, 11, 15];
            {
                _hostagePoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [14, 12, 3, 4, 5, 9, 10, 13];
   	    };
   	    case "Land_SCF_01_chimney_F": {
   	        {
                _supportPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [0, 1, 2, 3];
            {
                _sniperPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [4, 5, 6, 7, 8];
   	    };
   	    case "Land_dp_bigTank_F": {
   	        {
                _aaPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [0, 1, 6];
   	    };
   	    case "Land_MobileCrane_01_F": {
   	        {
                _supportPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [7, 8, 11]; 
   	    };
   	    case "Land_GuardHouse_01_F": {
   	        {
                _supportPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [0, 2, 4]; 
            {
                _hostagePoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [3, 5];
   	    };
   	    case "Land_Crane_F": {
            {
                _sniperPoses pushBack (AGLToASL (_building buildingPos _x));
            } forEach [1, 3];
   	    };
    };
} forEach _objects;

_supportPoses = _supportPoses call QS_fnc_TBshuffle;
_hostagePoses = _hostagePoses call QS_fnc_TBshuffle;
_aaPoses = _aaPoses call QS_fnc_TBshuffle;
_sniperPoses = _sniperPoses call QS_fnc_TBshuffle;
_viperPoses = _viperPoses call QS_fnc_TBshuffle;
_guardPoses = _guardPoses call QS_fnc_TBshuffle;

// add 9 hostages
_hostageGroup = createGroup civilian;
for "_c" from 0 to 8 do {
    (selectRandom [INFANTRY_HOSTAGES]) createUnit [[1,1,1], _hostageGroup, "currentHostage = this", 0, (selectRandom [INFANTRY_RANK])];
    currentHostage setVariable ["zbe_cacheDisabled", true, true];
    waitUntil {alive currentHostage};    
    currentHostage setPosASL (_hostagePoses select _c);
    currentHostage setDir (random 360);
    doStop currentHostage;
    commandStop currentHostage;
    _anim = selectRandom ["Acts_AidlPsitMstpSsurWnonDnon01","Acts_AidlPsitMstpSsurWnonDnon02","Acts_AidlPsitMstpSsurWnonDnon03","Acts_AidlPsitMstpSsurWnonDnon04","Acts_AidlPsitMstpSsurWnonDnon05","Acts_ExecutionVictim_Loop"];
    currentHostage switchMove _anim;
    currentHostage disableAI "ANIM";
    currentHostage addEventHandler ["killed", {SM_FAIL_RESCUE = SM_FAIL_RESCUE + 1;publicVariable "SM_FAIL_RESCUE";[(_this select 0),"QS_fnc_removeAction0",nil,true] spawn BIS_fnc_MP;(_this select 0) removeWeapon "hgun_Rook40_F";}];
    [currentHostage, "QS_fnc_addActionRescue",nil,true] spawn BIS_fnc_MP;
};
_hostageGroup setVariable ["zbe_cacheDisabled", true, true];

// add 6 support guards
_guardsGroup = createGroup ENEMY_SIDE;
for "_c" from 0 to 5 do {
    _spawnPos = _guardPoses select _c;
    (selectRandom [INFANTRY_SUPPORT]) createUnit [[1,1,1], _guardsGroup, "currentGuard = this", 0, (selectRandom [INFANTRY_RANK])];
    currentGuard setVariable ["zbe_cacheDisabled", true, true];
    waitUntil {alive currentGuard};    
    currentGuard setPosASL _spawnPos;
    currentGuard setDir (random 360);
    currentGuard setUnitPos "UP";
    doStop currentGuard;
    commandStop currentGuard;
    [currentGuard,(selectRandom ["WATCH","WATCH1","WATCH2"]),"FULL", {!isNull (currentGuard findNearestEnemy (getPos currentGuard)) || lifestate currentGuard == "INJURED"}, "COMBAT"] call BIS_fnc_ambientAnimCombat;
};
_guardsGroup setBehaviour "SAFE";
_guardsGroup setCombatMode "RED";
[(units _guardsGroup)] call QS_fnc_setSkill3;
_guardsGroup setVariable ["zbe_cacheDisabled", true, true];
_enemiesArray = _enemiesArray + (units _guardsGroup);

// add 8 AA soldiers on roof
_aaGroup = createGroup ENEMY_SIDE;
for "_c" from 0 to 5 do {
    _spawnPos = _aaPoses select _c;
    (selectRandom [INFANTRY_HOUSE]) createUnit [[1,1,1], _aaGroup, "currentGuard = this", 0, (selectRandom [INFANTRY_RANK])];
    currentGuard setVariable ["zbe_cacheDisabled", true, true];
    waitUntil {alive currentGuard};    
    currentGuard setPosASL _spawnPos;
    currentGuard setDir (random 360);
    currentGuard setUnitPos "UP";
    doStop currentGuard;
    commandStop currentGuard;
    [currentGuard,(selectRandom ["WATCH","WATCH1","WATCH2"]),"FULL", {!isNull (currentGuard findNearestEnemy (getPos currentGuard)) || lifestate currentGuard == "INJURED"}, "COMBAT"] call BIS_fnc_ambientAnimCombat;
};
_aaGroup setBehaviour "SAFE";
_aaGroup setCombatMode "RED";
[(units _aaGroup)] call QS_fnc_setSkill3;
_aaGroup setVariable ["zbe_cacheDisabled", true, true];
_enemiesArray = _enemiesArray + (units _aaGroup);

// add 1 viper team
_viperGroup = createGroup ENEMY_SIDE;
for "_c" from 0 to 7 do {
    _spawnPos = _viperPoses select _c;
    (selectRandom [INFANTRY_VIPER]) createUnit [[0,0,0], _viperGroup, "currentGuard = this", 0, (selectRandom [INFANTRY_RANK])];
    currentGuard setVariable ["zbe_cacheDisabled", true, true];
    waitUntil {alive currentGuard};    
    currentGuard setPosASL _spawnPos;
    currentGuard setDir (random 360);
    currentGuard setUnitPos "UP";
    doStop currentGuard;
    commandStop currentGuard;
    [currentGuard,(selectRandom ["WATCH","WATCH1","WATCH2"]),"FULL", {!isNull (currentGuard findNearestEnemy (getPos currentGuard)) || lifestate currentGuard == "INJURED"}, "COMBAT"] call BIS_fnc_ambientAnimCombat;
};
_viperGroup setBehaviour "SAFE";
_viperGroup setCombatMode "RED";
[(units _viperGroup)] call QS_fnc_setSkill3;
_viperGroup setVariable ["zbe_cacheDisabled", true, true];
_enemiesArray = _enemiesArray + (units _viperGroup);

// add 3 snipers
_sniperGroup = createGroup ENEMY_SIDE;
for "_c" from 0 to 2 do {
    _spawnPos = _sniperPoses select _c;
    (selectRandom [INFANTRY_SNIPERS]) createUnit [[0,0,0], _viperGroup, "currentGuard = this", 0, (selectRandom [INFANTRY_RANK])];
    currentGuard setVariable ["zbe_cacheDisabled", true, true];
    waitUntil {alive currentGuard};
    currentGuard setPosASL _spawnPos;
    currentGuard setDir (random 360);
    currentGuard setUnitPos "UP";
    doStop currentGuard;
    commandStop currentGuard;
    [currentGuard,(selectRandom ["WATCH","WATCH1","WATCH2"]),"FULL", {!isNull (currentGuard findNearestEnemy (getPos currentGuard)) || lifestate currentGuard == "INJURED"}, "COMBAT"] call BIS_fnc_ambientAnimCombat;
};
_sniperGroup setBehaviour "SAFE";
_sniperGroup setCombatMode "RED";
[(units _sniperGroup)] call QS_fnc_setSkill3;
_sniperGroup setVariable ["zbe_cacheDisabled", true, true];
_enemiesArray = _enemiesArray + (units _sniperGroup);

// set zone area
_startPoint = [(_flatPos select 0), (_flatPos select 1), 1];
{ _x setMarkerPos _flatPos; } forEach ["sideMarker", "sideCircle"];
"sideCircle" setMarkerSize [200, 200]; publicVariable "sideCircle";
sideMarkerText = [INFANTRY_MISSION, true]; publicVariable "sideMarkerText";
"sideMarker" setMarkerText INFANTRY_MISSION; publicVariable "sideMarker";

// show brief information
_briefing = format ["<t align='center'><t size='2.2'>Спецоперация</t><br/><t size='1.5' color='#FFC107'>%1</t><br/>____________________<br/>Противник захватил буровую вышку в нейтральных водах взяв персонал в заложники. Необходимо вернуть буровую под нашу опеку и освободить технический персонал. Командование назначило пехотную спецоперацию.</t>", "Нефть-Матушка"];
GlobalHint = _briefing; hint parseText GlobalHint; publicVariable "GlobalHint";
showNotification = ["NewSpecMission", "Нефть-Матушка"]; publicVariable "showNotification";
sideMissionUp = true; publicVariable "sideMissionUp";
SM_FAIL = false; publicVariable "SM_FAIL";
SM_FAIL_RESCUE = 0; publicVariable "SM_FAIL_RESCUE";
SM_SUCCESS_RESCUE = 0; publicVariable "SM_SUCCESS_RESCUE";
SM_SUCCESS_HOSTAGES = false; publicVariable "SM_SUCCESS_HOSTAGES";
_showHostagesMessage = true;
[_startPoint, 200, ["fire"]] call QS_fnc_addHades;

// save info in DB
try {
    _position = format ["%1,%2", floor (_flatPos select 0), floor (_flatPos select 1)];
    ["setInfo",["spec_name", "Нефть-Матушка"], 0] remoteExec ["sqlServerCall", 2];
    ["setInfo",["spec_position", _position], 0] remoteExec ["sqlServerCall", 2];
} catch {};

while { sideMissionUp } do {

	// hostages done
    if (_showHostagesMessage && {(SM_FAIL_RESCUE + SM_SUCCESS_RESCUE) > 8}) then {
        if (SM_SUCCESS_RESCUE >= 5) then {
            SM_SUCCESS_HOSTAGES = true;
            hqSideChat = format ["%1 из 9 заложников спасены!", SM_SUCCESS_RESCUE];
            publicVariable "hqSideChat"; [OUR_SIDE,"HQ"] sideChat hqSideChat;
        };
        if (SM_FAIL_RESCUE > 4) then {
            hqSideChat = format ["%1 из 9 заложников погибли!", SM_FAIL_RESCUE];
            publicVariable "hqSideChat"; [OUR_SIDE,"HQ"] sideChat hqSideChat;
            SM_FAIL = true; publicVariable "SM_FAIL";
        };
        _showHostagesMessage = false;
    };
    sleep 1;

    // de-briefing
    if (SM_SUCCESS_HOSTAGES || SM_FAIL) exitWith {
        sideMissionUp = false; publicVariable "sideMissionUp";
        { _x setMarkerPos [-12000,-12000,-12000]; publicVariable _x; } forEach ["sideMarker", "sideCircle"];
        "sideCircle" setMarkerSize [300, 300]; publicVariable "sideCircle";
        "sideMarker" setMarkerText ""; publicVariable "sideMarker";
        if (SM_FAIL) then {
            [true] call QS_fnc_SMhintFAIL;
        } else {
            if (WIN_WEST > WIN_GUER) then {
                [4] spawn QS_fnc_bluforSUCCESS;
            } else {
                [4] spawn QS_fnc_partizanSUCCESS;
            };
        };
        sleep 600;
        { [_x] call QS_fnc_TBdeleteObjects; } forEach [_enemiesArray, _objectsArray];
        [_flatPos, 500] call QS_fnc_DeleteEnemyEAST;
    };
    sleep 10;
};