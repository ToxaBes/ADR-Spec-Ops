// show brief information
_briefing = format ["<t align='center'><t size='2.2'>Спецоперация</t><br/><t size='1.5' color='#FFC107'>%1</t><br/>____________________<br/>Противник захватил буровую вышку в нейтральных водах взяв персонал в заложники. Необходимо вернуть буровую под нашу опеку и освободить технический персонал. Командование назначило пехотную спецоперацию.</t>", "Нефть-Матушка"];
GlobalHint = _briefing; hint parseText GlobalHint; publicVariable "GlobalHint";
showNotification = ["NewSpecMission", "Нефть-Матушка"]; publicVariable "showNotification";
sideMissionUp = true; publicVariable "sideMissionUp";
SM_FAIL = false; publicVariable "SM_FAIL";
SM_FAIL_RESCUE = 0; publicVariable "SM_FAIL_RESCUE";
SM_SUCCESS_RESCUE = 0; publicVariable "SM_SUCCESS_RESCUE";
SM_SUCCESS_HOSTAGES = false; publicVariable "SM_SUCCESS_HOSTAGES";
_showHostagesMessage = true;
[_startPoint, 200, ["fire"]] call QS_fnc_addHades;

// save info in DB
try {
    _position = format ["%1,%2", floor (_flatPos select 0), floor (_flatPos select 1)];
    ["setInfo",["spec_name", "Нефть-Матушка"], 0] remoteExec ["sqlServerCall", 2];
    ["setInfo",["spec_position", _position], 0] remoteExec ["sqlServerCall", 2];
} catch {};

while { sideMissionUp } do {

	// hostages done
    if (_showHostagesMessage && {(SM_FAIL_RESCUE + SM_SUCCESS_RESCUE) > 8}) then {
        if (SM_SUCCESS_RESCUE >= 5) then {
            SM_SUCCESS_HOSTAGES = true;
            hqSideChat = format ["%1 из 9 заложников спасены!", SM_SUCCESS_RESCUE];
            publicVariable "hqSideChat"; [OUR_SIDE,"HQ"] sideChat hqSideChat;
        };
        if (SM_FAIL_RESCUE > 4) then {
            hqSideChat = format ["%1 из 9 заложников погибли!", SM_FAIL_RESCUE];
            publicVariable "hqSideChat"; [OUR_SIDE,"HQ"] sideChat hqSideChat;
            SM_FAIL = true; publicVariable "SM_FAIL";
        };
        _showHostagesMessage = false;
    };
    sleep 1;

    // de-briefing
    if (SM_SUCCESS_HOSTAGES || SM_FAIL) exitWith {
        sideMissionUp = false; publicVariable "sideMissionUp";
        { _x setMarkerPos [-12000,-12000,-12000]; publicVariable _x; } forEach ["sideMarker", "sideCircle"];
        "sideCircle" setMarkerSize [300, 300]; publicVariable "sideCircle";
        "sideMarker" setMarkerText ""; publicVariable "sideMarker";
        if (SM_FAIL) then {
            [true] call QS_fnc_SMhintFAIL;
        } else {
            if (WIN_WEST > WIN_GUER) then {
                [4] spawn QS_fnc_bluforSUCCESS;
            } else {
                [4] spawn QS_fnc_partizanSUCCESS;
            };
        };
        sleep 600;
        { [_x] call QS_fnc_TBdeleteObjects; } forEach [_enemiesArray, _objectsArray];
        [_flatPos, 500] call QS_fnc_DeleteEnemyEAST;
    };
    sleep 10;
};